# System Mode:1:Router,2:AP,3:Repeater,4:WISP,5:WISP+UR

config system main
	option release_year 2016
	option product_name NBG6817
	option product_typ_name "ARMOR Z2"
	option product_model DUAL_BAND
	option hostname	NBG6817
	option weather_city TWXX0021
	option weather_degree c
	option domain_name local
	option pwd 1234
	option power_saving_select '2.4G'
	option language "en"
	option systimeout 300
	option klogconloglevel 1
	option log_size 128
	option system_mode 1
	option timezone UTC
	option changeDefaultCg 4
	option fw_upgrade_time 200
	option gui_restart_time 140

config rdate
	option interface wan
	
config LED led
	option on '1'

config cloud cloud
	option do_unpair 1
